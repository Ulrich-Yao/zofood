<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <title>Hello, world!</title>
</head>

<body>
    <div class="card">
        <div class="card-header">
            <h1 class="text-center">ENREGISTREMENT DES VEHICULES</h1>
        </div>
        <div class="card-body">
            <div class="container">
                <div class="row">
                    <div class="col-8">
                        <form class="row g-3">
                            <div class="col-md-3">
                                <label for="validationDefault05" class="form-label">Photo carte grise</label>
                                <input type="file" accept="image/*" capture class="form-control" id="validationDefault05" required>
                            </div>
                            <div class="col-md-3">
                                <label for="validationDefault03" class="form-label">Photo véhicule avant</label>
                                <input type="file" accept="image/*" capture class="form-control" id="validationDefault03" required>
                            </div>
                            <div class="col-md-3">
                                <label for="validationDefault03" class="form-label">Photo véhicule arrière</label>
                                <input type="file" accept="image/*" capture class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>
                            <div class="col-md-3">
                                <label for="validationDefault03" class="form-label">Photo véhicule coté</label>
                                <input type="file" accept="image/*" capture class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault01" class="form-label">Numéro d'immatriculation</label>
                                <input type="text" class="form-control" id="validationDefault01" name="immatriculation" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault02" class="form-label">Numéro carte grise</label>
                                <input type="text" class="form-control" id="validationDefault02" name="grise" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefaultUsername" class="form-label">Date de première mise en service</label>
                                <div class="input-group">
                                    <input type="date" class="form-control" id="validationDefault01" name="miseenservice" required>

                                </div>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault03" class="form-label">Date d'édiction de la carte</label>
                                <input type="date" class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault03" class="form-label">Identité du propriétaire</label>
                                <input type="text" class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>

                            <div class="col-md-4">
                                <label for="validationDefault05" class="form-label">Marque</label>
                                <input type="text" class="form-control" id="validationDefault05" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault03" class="form-label">Type commercial</label>
                                <input type="text" class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault03" class="form-label">Couleur</label>
                                <input type="text" class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>

                            <div class="col-md-4">
                                <label for="validationDefault05" class="form-label">Carosserie</label>
                                <input type="text" class="form-control" id="validationDefault05" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault03" class="form-label">Place assise</label>
                                <input type="number" class="form-control" id="validationDefault03" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault04" class="form-label">Energie</label>
                                <select class="form-select" id="validationDefault04" required>
                                  <option selected disabled value="">Choisir</option>
                                  <option>Gasoil</option>
                                  <option>Essence</option>
                                  <option>Electrique</option>

                                </select>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault05" class="form-label">PTAC (kg)</label>
                                <input type="number" class="form-control" id="validationDefault05" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault03" class="form-label">Usage</label>
                                <input type="text" class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>

                            <div class="col-md-4">
                                <label for="validationDefault05" class="form-label">Puissance fiscale PV</label>
                                <input type="number" class="form-control" id="validationDefault05" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault03" class="form-label">Nombre d'essieux</label>
                                <input type="number" class="form-control" id="validationDefault03" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault03" class="form-label">Cylindre</label>
                                <input type="number" class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault03" class="form-label">PV</label>
                                <input type="number" class="form-control" id="validationDefault03" required>
                            </div>
                            <div class="col-md-4">
                                <label for="validationDefault03" class="form-label">Genre</label>
                                <input type="number" class="form-control" id="validationDefault03" required>
                            </div>



                            <div class="col-md-3">
                                <label for="validationDefault05" class="form-label">Assurance: Souscription</label>
                                <input type="date" class="form-control" id="validationDefault05" required>
                            </div>
                            <div class="col-md-3">
                                <label for="validationDefault03" class="form-label">Assurance: Expiration</label>
                                <input type="date" class="form-control" id="validationDefault03" required>
                            </div>
                            <div class="col-md-3">
                                <label for="validationDefault03" class="form-label">Type d'assurance</label>
                                <input type="text" class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>
                            <div class="col-md-3">
                                <label for="validationDefault03" class="form-label">Assureur</label>
                                <input type="text" class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>
                            <div class="col-md-3">
                                <label for="validationDefault05" class="form-label">Vignette: Souscription</label>
                                <input type="date" class="form-control" id="validationDefault05" required>
                            </div>
                            <div class="col-md-3">
                                <label for="validationDefault03" class="form-label">Vignette: Expiration</label>
                                <input type="date" class="form-control" id="validationDefault03" required>
                            </div>
                            <div class="col-md-3">
                                <label for="validationDefault03" class="form-label">Type de vignette</label>
                                <input type="text" class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>
                            <div class="col-md-3">
                                <label for="validationDefault03" class="form-label">Entreprise</label>
                                <input type="text" class="form-control" id="validationDefault01" name="dateedition" required>
                            </div>

                            <div class="col-12">

                            </div>
                            <div class="col-12">
                                <button class="btn btn-primary" type="submit">Enregistrer</button>
                            </div>
                        </form>
                    </div>
                    <div class="col-3 mr-3">

                    </div>
                </div>

            </div>
        </div>
    </div>

    <!-- Optional JavaScript; choose one of the two! -->

    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

    <!-- Option 2: Separate Popper and Bootstrap JS -->
